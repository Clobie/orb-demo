script/source = "extends CanvasLayer

@onready var button_singleplayer = $MarginContainer/VBoxContainer/Button_Singleplayer
@onready var button_online = $MarginContainer/VBoxContainer/Button_Online
@onready var button_settings = $MarginContainer/VBoxContainer/Button_Settings
@onready var button_credits = $MarginContainer/VBoxContainer/Button_Credits
@onready var button_quit = $MarginContainer/VBoxContainer/Button_Quit
@onready var label_version = $MarginContainer2/Label_Version
@onready var sprite_2d = $Sprite2D

var dir = Vector2()

func _ready():
	dir = Vector2(randf(), randf())

func _process(delta):
	pass

func _on_button_singleplayer_pressed():
	get_tree().change_scene_to_file(\"res://scenes/worlds/example_level/level.tscn\")

func _on_button_online_pressed():
	pass

func _on_button_settings_pressed():
	pass

func _on_button_credits_pressed():
	get_tree().change_scene_to_file(\"res://scenes/menus/credits_menu/credit_menu.tscn\")

func _on_button_quit_pressed():
	get_tree().quit()
"

